<Project Sdk="Microsoft.NET.Sdk">


  <Import Project="../../../targets/lib-vault-standard.targets" />

  <PropertyGroup>
    <Description>Redis client used for easy interaction with Redis servers.</Description>
	
	<GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="StackExchange.Redis" Version="1.2.6" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="6.0.0" />
  </ItemGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>$(AssemblyName).Tests</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="../../configuration/configuration/Configuration.csproj" />
    <ProjectReference Include="../../logging/logging/Logging.csproj" />
    <ProjectReference Include="../../hashing/hashing/Hashing.csproj" />
    <ProjectReference Include="../../instrumentation/instrumentation/Instrumentation.csproj" />
    <ProjectReference Include="../../service-discovery/service-discovery/ServiceDiscovery.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties/HybridRedisClientProviderSettings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>HybridRedisClientProviderSettings.settings</DependentUpon>
    </Compile>
    <Compile Update="Properties/SelfHealingConnectionMultiplexerSettings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>SelfHealingConnectionMultiplexerSettings.settings</DependentUpon>
    </Compile>
    <Compile Update="Properties/Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>

    <Compile Update="Properties/Settings.cs">
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
    <Compile Update="Properties/SelfHealingConnectionMultiplexerSettings.cs">
      <DependentUpon>SelfHealingConnectionMultiplexerSettings.settings</DependentUpon>
    </Compile>
    <Compile Update="Properties/ISelfHealingConnectionMultiplexerSettings.cs">
      <DependentUpon>SelfHealingConnectionMultiplexerSettings.settings</DependentUpon>
    </Compile>
    <Compile Update="Properties/HybridRedisClientProviderSettings.cs">
      <DependentUpon>HybridRedisClientProviderSettings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties/HybridRedisClientProviderSettings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>HybridRedisClientProviderSettings.Designer.cs</LastGenOutput>
    </None>
    <None Update="Properties/SelfHealingConnectionMultiplexerSettings.settings">
      <Generator>PublicSettingsSingleFileGenerator</Generator>
      <LastGenOutput>SelfHealingConnectionMultiplexerSettings.Designer.cs</LastGenOutput>
    </None>
    <None Update="Properties/Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
