{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Grid.Bot",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/bin/debug/net6.0/Grid.Bot.dll",
            "cwd": "${workspaceFolder}/src",
            "console": "internalConsole",
            "stopAtEntry": false,
            "logging": {
                "moduleLoad": false
            },
            "requireExactSource": false,
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}