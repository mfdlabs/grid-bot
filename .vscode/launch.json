{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Grid.Bot",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/bin/debug/net8.0/Grid.Bot.dll",
            "cwd": "${workspaceFolder}/src",
            "console": "externalTerminal",
            "stopAtEntry": false,
            "logging": {
                "moduleLoad": false,
                "programOutput": true
            },
            "requireExactSource": false,
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Grid.Bot (Local)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-local",
            "program": "${workspaceFolder}/src/bin/debug/net8.0/Grid.Bot.dll",
            "cwd": "${workspaceFolder}/src",
            "console": "externalTerminal",
            "stopAtEntry": false,
            "logging": {
                "moduleLoad": false,
                "programOutput": true
            },
            "requireExactSource": false,
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}